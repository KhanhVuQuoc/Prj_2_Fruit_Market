<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="ProductPane" fx:id="ProductPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.fruit.market.ProductController">
   <children>
      <HBox alignment="TOP_CENTER" prefHeight="87.0" prefWidth="529.0">
         <children>
            <ImageView fx:id="Pic" fitHeight="69.0" fitWidth="63.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin>
               <image>
                  <Image url="@../../../../values/img/coconut.png" />
               </image>
            </ImageView>
            <Label fx:id="lbfname" prefHeight="21.0" prefWidth="77.0" text="Coconut">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" top="30.0" />
               </HBox.margin>
            </Label>
            <Button  fx:id="incre" mnemonicParsing="false" text="+">
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </Button>
            <Label fx:id="lbamount" alignment="CENTER" prefHeight="21.0" prefWidth="60.0" text="20">
               <HBox.margin>
                  <Insets left="5.0" top="30.0" />
               </HBox.margin>
            </Label>
            <Button  fx:id="decre" mnemonicParsing="false" text="-">
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </Button>
            <Button onAction="#handleDelete" fx:id="dele" mnemonicParsing="false" text="X">
               <HBox.margin>
                  <Insets left="5.0" top="25.0" />
               </HBox.margin>
            </Button>
            <Label fx:id="lbprice" text="\$3.99">
               <HBox.margin>
                  <Insets left="5.0" top="30.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
